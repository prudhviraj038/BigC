<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:id="@+id/content_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/hhh"
                android:src="@drawable/dashboardbackground"
                android:scaleType="centerCrop" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout2"
                android:layout_marginTop="50dp" >

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView29"
                        android:src="@drawable/bigclogo"
                        android:adjustViewBounds="true"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:visibility="invisible" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView30"
                        android:src="@drawable/bigclogo"
                        android:adjustViewBounds="true"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView33"
                        android:src="@drawable/bigclogo"
                        android:adjustViewBounds="true"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:visibility="invisible" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:divider="@drawable/dashboard_whitedivider"
                    android:showDividers="middle|beginning|end"
                    android:paddingTop="80dp" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/employee"
                        android:src="@drawable/redcircle"
                        android:adjustViewBounds="true"
                        android:layout_weight="1" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/customer"
                        android:src="@drawable/bluecircle"
                        android:adjustViewBounds="true"
                        android:layout_weight="1" />
                </LinearLayout>
            </LinearLayout>

            <ImageView
                android:layout_width="50dp"
                android:layout_height="60dp"
                android:id="@+id/imageView44"
                android:src="@drawable/headericon"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="10dp"
                android:adjustViewBounds="false" />
        </RelativeLayout>
    </FrameLayout>
    <LinearLayout
        android:layout_gravity="start"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linearLayout2"
        android:layout_marginRight="100dp"
        android:gravity="start"
        android:background="#ffffff"
        android:divider="@drawable/nav_divider"
        android:showDividers="beginning|middle|end">


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:padding="10dp">

            <ImageView
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/imageView7"
                android:src="@drawable/home_icon"
                android:layout_marginLeft="50dp"
                android:layout_marginRight="25dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Home"
                android:id="@+id/textView11"
                android:textSize="40sp"
                android:textStyle="bold"
                android:textColor="#000000" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical" >

            <ImageView
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:id="@+id/imageView28" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Home"
                android:id="@+id/textView36"
                android:textSize="25sp"
                android:textStyle="bold"
                android:textColor="#2f92df" />
        </LinearLayout>

    </LinearLayout>



</android.support.v4.widget.DrawerLayout>